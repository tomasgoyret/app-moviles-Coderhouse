{
  "expo": {
    "name": "Checked",
    "slug": "Checked",
    "version": "1.0.0",
    "platforms": ["ios","android"],
    "orientation": "portrait",
    "icon": "./assets/edddd4.png",
    "splash": {
      "image": "./assets/edddd4.png",
      "resizeMode": "contain",
      "backgroundColor": "#283d3b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.codehouse.checked",
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/edddd4.png",
        "backgroundColor": "#283d3b"
      },
      "permissions": ["WRITE_EXTERNAL_STORAGE"]
    },
  
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
